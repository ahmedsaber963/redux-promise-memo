{
  "name": "redux-promise-memo",
  "version": "0.2.1",
  "description": "memoization for promise-based Redux action creators",
  "author": "saltycrane",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/saltycrane/redux-promise-memo.git"
  },
  "keywords": [
    "cache",
    "memoize",
    "memoization",
    "middleware",
    "promise",
    "redux"
  ],
  "main": "./lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "flow": "flow version && flow --include-warnings",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "prettier": "prettier --write 'src/**/**.js'",
    "test": "jest --testPathIgnorePatterns examples"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "parser": "flow",
    "trailingComma": "all"
  },
  "peerDependencies": {
    "redux": ">=3.1.0",
    "redux-thunk": ">=1.0.0"
  },
  "//": [
    "redux and redux-thunk are included in devDependencies for unit tests"
  ],
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-flow": "6.23.0",
    "flow-bin": "0.73.0",
    "jest": "21.2.1",
    "prettier": "1.13.4",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0"
  }
}
